<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/round_frame_cehicles">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/dp_15"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/qiu" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:text="# 2"
                    android:textColor="@android:color/black"
                    android:textSize="@dimen/sp_16" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_2"
                android:layout_marginTop="@dimen/dp_12"
                android:background="#ECEDED" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ccd_data0_B"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv2_ccd_data0_b"
                    android:layout_width="@dimen/dp_140"
                    android:layout_height="@dimen/dp_33"
                    android:layout_marginLeft="@dimen/dp_50"
                    android:background="@drawable/btn_round_gray"
                    android:gravity="center"
                    android:textColor="#999999"
                    android:textSize="@dimen/sp_13" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_2"
                android:background="#ECEDED" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ccd_data1_B"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv2_ccd_data1_b"
                    android:layout_width="@dimen/dp_140"
                    android:layout_height="@dimen/dp_33"
                    android:layout_marginLeft="@dimen/dp_50"
                    android:background="@drawable/btn_round_gray"
                    android:gravity="center"
                    android:textColor="#999999"
                    android:textSize="@dimen/sp_13" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_2"
                android:background="#ECEDED" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Roll_B"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv2_roll_b"
                    android:layout_width="@dimen/dp_140"
                    android:layout_height="@dimen/dp_33"
                    android:layout_marginLeft="@dimen/dp_90"
                    android:background="@drawable/btn_round_gray"
                    android:gravity="center"
                    android:textColor="#999999"
                    android:textSize="@dimen/sp_13" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_2"
                android:background="#ECEDED" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Pitch_B"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv2_pitch_b"
                    android:layout_width="@dimen/dp_140"
                    android:layout_height="@dimen/dp_33"
                    android:layout_marginLeft="@dimen/dp_81"
                    android:background="@drawable/btn_round_gray"
                    android:gravity="center"
                    android:textColor="#999999"
                    android:textSize="@dimen/sp_13" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_2"
                android:background="#ECEDED" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Yaw_B"
                    android:textSize="@dimen/sp_13" />

                <TextView
                    android:id="@+id/tv2_yaw_b"
                    android:layout_width="@dimen/dp_140"
                    android:layout_height="@dimen/dp_33"
                    android:layout_marginLeft="@dimen/dp_86"
                    android:background="@drawable/btn_round_gray"
                    android:gravity="center"
                    android:textColor="#999999"
                    android:textSize="@dimen/sp_13" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_2"
                android:background="#ECEDED" />
        </LinearLayout>

    </LinearLayout>
</LinearLayout>
